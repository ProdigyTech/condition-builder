{
  "compilerOptions": {
    "composite": true,
    "baseUrl": "./src",
    "skipLibCheck": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "jsx": "react-jsx",
    "paths": {
      "@Components": ["Components/index"],
      "@Context/*": ["Context/*"],
      "@Shared": ["Shared/index"],
      "@Utils": ["utils/index"]
    }
  },
  "include": ["vite.config.ts", "src"]
}
